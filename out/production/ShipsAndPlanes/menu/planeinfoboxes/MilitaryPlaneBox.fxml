<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="425.0" prefWidth="275.0" xmlns="http://javafx.com/fxml" xmlns:fx="http://javafx.com/fxml/1" fx:controller="menu.planeinfoboxes.MilitaryPlaneBoxController">
    <children>
        <Label fx:id="idLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="60.0" layoutY="18.0" prefHeight="30.0" prefWidth="150.0" text="Label" textAlignment="CENTER">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button fx:id="deleteButton" alignment="CENTER" contentDisplay="CENTER" layoutX="153.0" layoutY="354.0" mnemonicParsing="false" onAction="#deletePlane" prefHeight="45.0" prefWidth="95.0" text="Delete plane" textAlignment="CENTER" wrapText="true">
            <font>
                <Font size="13.0" />
            </font></Button>
        <Button layoutX="29.0" layoutY="354.0" mnemonicParsing="false" onAction="#emergencyLanding" prefHeight="45.0" prefWidth="95.0" text="Emergency landing" textAlignment="CENTER" wrapText="true" />
        <ListView fx:id="flightPlanListView" layoutX="21.0" layoutY="204.0" prefHeight="92.0" prefWidth="110.0" />
        <Label alignment="CENTER" layoutX="68.0" layoutY="171.0" prefHeight="20.0" prefWidth="138.0" text="Change flight plan">
            <font>
                <Font size="14.0" />
            </font></Label>
        <ChoiceBox fx:id="airportChoiceBox" layoutX="146.0" layoutY="204.0" onAction="#addAirportToFlightPlan" prefHeight="26.0" prefWidth="110.0" />
        <Button alignment="CENTER" layoutX="157.0" layoutY="250.0" mnemonicParsing="false" onAction="#removeLastAirport" prefHeight="46.0" prefWidth="86.0" text="Remove last entry" textAlignment="CENTER" wrapText="true" />
        <Button alignment="CENTER" layoutX="164.0" layoutY="306.0" mnemonicParsing="false" onAction="#clearFlightPlan" prefHeight="26.0" prefWidth="75.0" text="Clear list" textAlignment="CENTER" />
        <Button alignment="CENTER" layoutX="33.0" layoutY="306.0" mnemonicParsing="false" onAction="#setNewFlightPlan" prefHeight="26.0" prefWidth="86.0" text="Set new plan" textAlignment="CENTER" />
        <Label alignment="CENTER" layoutX="87.0" layoutY="48.0" prefHeight="18.0" prefWidth="95.0" text="Military aircraft" textAlignment="CENTER" />
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="30.0" layoutY="74.0" prefHeight="20.0" prefWidth="75.0" text="Flying to: " textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="locationLabel" alignment="TOP_CENTER" layoutX="7.0" layoutY="99.0" prefHeight="39.0" prefWidth="122.0" text="*location*" wrapText="true">
            <font>
                <Font name="System Italic" size="13.0" />
            </font>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="146.0" layoutY="71.0" prefHeight="26.0" prefWidth="110.0" text="Weapon type:" textAlignment="CENTER" wrapText="true">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="armamentLabel" alignment="TOP_CENTER" contentDisplay="CENTER" layoutX="132.0" layoutY="99.0" prefHeight="39.0" prefWidth="138.0" text="*armament*" textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="System Italic" size="13.0" />
            </font>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="145.0" prefHeight="20.0" prefWidth="75.0" text="Staff:" textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="staffLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="64.0" layoutY="145.0" prefHeight="18.0" prefWidth="50.0" text="*staff*" textAlignment="CENTER">
            <font>
                <Font size="13.0" />
            </font>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="144.0" layoutY="145.0" prefHeight="20.0" prefWidth="75.0" text="Fuel: " textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font></Label>
        <Label fx:id="fuelLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="201.0" layoutY="145.0" prefHeight="18.0" prefWidth="50.0" text="*fuel*" textAlignment="CENTER">
            <font>
                <Font size="13.0" />
            </font></Label>
    </children>

</AnchorPane>
