<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="450.0" prefWidth="300.0" xmlns="http://javafx.com/fxml" xmlns:fx="http://javafx.com/fxml/1" fx:controller="menu.planecreationboxes.AddPassengerPlaneBoxController">
    <children>
        <Button layoutX="80.0" layoutY="380.0" mnemonicParsing="false" onAction="#addPlane" prefHeight="42.0" prefWidth="136.0" text="Create plane">
            <font>
                <Font size="16.0" />
            </font></Button>
        <TextField fx:id="idField" layoutX="29.0" layoutY="102.0" prefHeight="26.0" prefWidth="102.0" />
        <Label contentDisplay="CENTER" layoutX="42.0" layoutY="74.0" text="Name (ID):">
            <font>
                <Font size="16.0" />
            </font></Label>
        <ListView fx:id="flightPlanListView" layoutX="21.0" layoutY="265.0" prefHeight="102.0" prefWidth="110.0" />
        <ChoiceBox fx:id="airportChoiceBox" layoutX="152.0" layoutY="263.0" onAction="#addAirportToFlightPlan" prefHeight="26.0" prefWidth="128.0" />
        <Button layoutX="177.0" layoutY="340.0" mnemonicParsing="false" onAction="#clearFlightPlan" prefHeight="26.0" prefWidth="78.0" text="Clear list" />
        <Label contentDisplay="CENTER" layoutX="80.0" layoutY="225.0" prefHeight="26.0" prefWidth="136.0" text="Choose flight plan">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Button layoutX="157.0" layoutY="303.0" mnemonicParsing="false" onAction="#removeLastAirport" prefHeight="26.0" prefWidth="118.0" text="Remove last entry" textAlignment="CENTER" wrapText="true" />
        <Label layoutX="28.0" layoutY="152.0" text="Staff:">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Slider fx:id="staffSlider" layoutX="75.0" layoutY="166.0" prefHeight="6.0" prefWidth="66.0" />
        <Label fx:id="staffLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="94.0" layoutY="144.0" text="Label" />
        <Label alignment="CENTER" layoutX="20.0" layoutY="198.0" prefHeight="20.0" prefWidth="46.0" text="Speed:" textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Slider fx:id="speedSlider" layoutX="75.0" layoutY="208.0" prefHeight="6.0" prefWidth="66.0" />
        <Label fx:id="speedLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="94.0" layoutY="189.0" text="Label" />
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="157.0" layoutY="74.0" prefHeight="26.0" prefWidth="118.0" text="Company:" textAlignment="CENTER" wrapText="true">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <ChoiceBox fx:id="companyChoiceBox" layoutX="157.0" layoutY="102.0" prefHeight="26.0" prefWidth="118.0" />
      <Label alignment="CENTER" layoutX="177.0" layoutY="152.0" prefHeight="20.0" prefWidth="78.0" text="Total seats:" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Slider fx:id="capacitySlider" layoutX="165.0" layoutY="196.0" prefHeight="14.0" prefWidth="102.0" />
      <Label fx:id="capacityLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="202.0" layoutY="173.0" text="Label" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="44.0" layoutY="24.0" prefHeight="36.0" prefWidth="208.0" text="Create Passenger Plane" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
    </children>

</AnchorPane>
