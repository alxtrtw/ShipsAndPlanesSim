<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="425.0" prefWidth="275.0" xmlns="http://javafx.com/fxml" xmlns:fx="http://javafx.com/fxml/1" fx:controller="menu.planeinfoboxes.PassengerPlaneBoxController">
    <children>
        <Label fx:id="idLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="60.0" layoutY="18.0" prefHeight="30.0" prefWidth="150.0" text="Label" textAlignment="CENTER">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button alignment="CENTER" contentDisplay="CENTER" layoutX="153.0" layoutY="354.0" mnemonicParsing="false" onAction="#deletePlane" prefHeight="45.0" prefWidth="95.0" text="Delete plane" textAlignment="CENTER" wrapText="true">
            <font>
                <Font size="13.0" />
            </font></Button>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="180.0" layoutY="122.0" prefHeight="20.0" prefWidth="75.0" text="Fuel: " textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font></Label>
        <Label fx:id="fuelLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="192.0" layoutY="145.0" prefHeight="18.0" prefWidth="50.0" text="*fuel*" textAlignment="CENTER">
         <font>
            <Font size="13.0" />
         </font></Label>
        <Button layoutX="29.0" layoutY="354.0" mnemonicParsing="false" onAction="#emergencyLanding" prefHeight="45.0" prefWidth="95.0" text="Emergency landing" textAlignment="CENTER" wrapText="true" />
