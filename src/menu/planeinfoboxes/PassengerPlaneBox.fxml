        <Label alignment="CENTER" layoutX="68.0" layoutY="177.0" prefHeight="20.0" prefWidth="138.0" text="Change flight plan">
            <font>
                <Font size="14.0" />
            </font></Label>
        <ChoiceBox fx:id="airportChoiceBox" layoutX="146.0" layoutY="207.0" onAction="#addAirportToFlightPlan" prefHeight="26.0" prefWidth="110.0" />
        <Button alignment="CENTER" layoutX="157.0" layoutY="253.0" mnemonicParsing="false" onAction="#removeLastAirport" prefHeight="39.0" prefWidth="86.0" text="Remove last" textAlignment="CENTER" wrapText="true" />
        <Button layoutX="164.0" layoutY="306.0" mnemonicParsing="false" onAction="#clearFlightPlan" prefHeight="26.0" prefWidth="75.0" text="Clear list" />
        <Button layoutX="33.0" layoutY="306.0" mnemonicParsing="false" onAction="#setNewFlightPlan" prefHeight="26.0" prefWidth="86.0" text="Set new plan" />
      <Label fx:id="companyLabel" alignment="CENTER" layoutX="66.0" layoutY="48.0" prefHeight="18.0" prefWidth="138.0" text="*Company* plane" textAlignment="CENTER" />
      <Label alignment="CENTER" layoutX="100.0" layoutY="74.0" prefHeight="20.0" prefWidth="75.0" text="Flying to: ">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="locationLabel" alignment="CENTER" layoutX="62.0" layoutY="94.0" prefHeight="20.0" prefWidth="150.0" text="*location*">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="100.0" layoutY="119.0" prefHeight="26.0" prefWidth="75.0" text="Passengers:" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="passengersLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="112.0" layoutY="145.0" prefHeight="18.0" prefWidth="50.0" text="*passgs*" textAlignment="CENTER">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="122.0" prefHeight="20.0" prefWidth="75.0" text="Staff:" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="staffLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="32.0" layoutY="145.0" prefHeight="18.0" prefWidth="50.0" text="*staff*" textAlignment="CENTER">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
    </children>

</AnchorPane>
